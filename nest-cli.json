{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/intercom-backend/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/intercom-backend/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/intercom-backend",
  "projects": {
    "intercom-backend": {
      "type": "application",
      "root": "apps/intercom-backend",
      "entryFile": "main",
      "sourceRoot": "apps/intercom-backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/intercom-backend/tsconfig.app.json"
      }
    },
    "chats-service": {
      "type": "application",
      "root": "apps/chats-service",
      "entryFile": "main",
      "sourceRoot": "apps/chats-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chats-service/tsconfig.app.json"
      }
    },
    "analytics": {
      "type": "application",
      "root": "apps/analytics",
      "entryFile": "main",
      "sourceRoot": "apps/analytics/src",
      "compilerOptions": {
        "tsConfigPath": "apps/analytics/tsconfig.app.json"
      }
    },
    "notifications": {
      "type": "application",
      "root": "apps/notifications",
      "entryFile": "main",
      "sourceRoot": "apps/notifications/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notifications/tsconfig.app.json"
      }
    },
    "databases": {
      "type": "library",
      "root": "libs/databases",
      "entryFile": "index",
      "sourceRoot": "libs/databases/src",
      "compilerOptions": {
        "tsConfigPath": "libs/databases/tsconfig.lib.json"
      }
    },
    "providers": {
      "type": "library",
      "root": "libs/providers",
      "entryFile": "index",
      "sourceRoot": "libs/providers/src",
      "compilerOptions": {
        "tsConfigPath": "libs/providers/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "errors": {
      "type": "library",
      "root": "libs/errors",
      "entryFile": "index",
      "sourceRoot": "libs/errors/src",
      "compilerOptions": {
        "tsConfigPath": "libs/errors/tsconfig.lib.json"
      }
    },
    "middleware": {
      "type": "library",
      "root": "libs/middleware",
      "entryFile": "index",
      "sourceRoot": "libs/middleware/src",
      "compilerOptions": {
        "tsConfigPath": "libs/middleware/tsconfig.lib.json"
      }
    },
    "guards": {
      "type": "library",
      "root": "libs/guards",
      "entryFile": "index",
      "sourceRoot": "libs/guards/src",
      "compilerOptions": {
        "tsConfigPath": "libs/guards/tsconfig.lib.json"
      }
    }
  }
}